{
  "version": 2,
  "public": false,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "./index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "/api/index.js" },
    { "src": "^/build/(.*)", "dest": "/build/$1" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
